{
  "name": "koa-auth-practice",
  "version": "1.0.0",
  "description": "koa auth with jwt",
  "main": "index.js",
  "repository": "https://github.com/KS-KIM/learning-node-js",
  "author": "K.S.KIM <nullable@kakao.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "build": "rimraf build && tsc",
    "migration:local:create": "yarn build && typeorm migration:create -f src/infra/config/local.ormconfig.json",
    "migration:local:generate": "yarn build && typeorm migration:generate -f src/infra/config/local.ormconfig.json",
    "migration:local:run": "yarn build && typeorm migration:run -f src/infra/config/local.ormconfig.json",
    "migration:local:revert": "yarn build && typeorm migration:revert -f src/infra/config/local.ormconfig.json"
  },
  "dependencies": {
    "@koa/router": "^10.0.0",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "mysql": "npm:mysql2@^2.1.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/koa": "^2.13.1",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
